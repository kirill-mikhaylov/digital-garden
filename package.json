{
  "name": "digital-garden",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "check-types": "turbo run check-types",
    "clean": "find . -name 'node_modules' -type d -exec rm -rf {} +",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "vercel:env": "turbo run vercel:env"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@10.15.1",
  "engines": {
    "node": ">=22"
  }
}
